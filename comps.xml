<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE comps PUBLIC "-//Red Hat, Inc.//DTD Comps info//EN" "comps.dtd">
<comps>
  <group>
    <id>flagship-product</id>
    <_name>Ultramarine Flagship product core</_name>
    <_description>Packages mandatory for the Ultramarine Flagship product.</_description>
    <default>false</default>
    <uservisible>false</uservisible>
    <packagelist>
        <packagereq type="mandatory">budgie-extras</packagereq>
        <packagereq type="mandatory">budgie-extras-daemon</packagereq>
        <packagereq type="default">ultramarine-backgrounds-common</packagereq>
        <packagereq type="default">ultramarine-flagship-filesystem</packagereq>
        <packagereq type="default">ultramarine-release-flagship</packagereq>
    </packagelist>
  </group>
  <environment>
    <id>flagship-product-enviroment</id>
    <_name>Ultramarine Flagship</_name>
    <_description>Ultramarine Flagship is the flagship Ultramarine experience.<_description/>
    <grouplist>
        <groupid>base-x</groupid>
        <groupid>standard</groupid>
        <groupid>core</groupid>
        <groupid>admin-tools</groupid>
        <groupid>dial-up</groupid>
        <groupid>fonts</groupid>
        <groupid>input-methods</groupid>
        <groupid>multimedia</groupid>
        <groupid>networkmanager-submodules</groupid>
        <groupid>hardware-support</groupid>
        <groupid>printing</groupid>
        <groupid>guest-desktop-agents</groupid>
        <groupid>budgie-desktop</groupid>
        <groupid>budgie-desktop-apps</groupid>
    </grouplist>
  </environment>
</comps>